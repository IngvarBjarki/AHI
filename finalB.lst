 430  PAP_HSEL(t)..  PAP_Pro*y("PAP",t) =l= y("HSEL",t);
 431  PAP_LSEL(t)..  PAP_Pro*y("PAP",t) =l= y("LSEL",t);
 432  PULP_Bal(p3,t) .. sum((l,k), u(l,p3,k,t)*q(l,p3)) + PAP_Pro*y("PAP",t) =l= y(P3,t);
 433   
 434  // =========ADD FIXED COST FOR INCREASED CAPACITY========== //
 435   
 436   
 437   
 438  // =====PROFIT(OLD OBJECTIVE FUNCTION)=======//
 439   
 440  nPROFIT(t).. Pr(t) =e=  (sum((k,j), (GAMMA(j,k)/1000) * sum(l, q(l,j)*u(l,j,k,t)))
 441  - sum((k,j), (DELTA(j,k)/(1000*1000)) * sum(l, q(l,j)*q(l,j) * u(l,j,k,t))/power(demand_growth(j), ord(t)-1)))   //Amount sold times sellingprice
 442   
 443                      - sum(i, ALPHA(i)/1000 * sum(n, h(n,i)*r(n,i,t))) - sum(i, BETA(i)/(1000*1000) * sum(n, h(n,i)*h(n,i) * r(n,i,t)))                    //Amount bought times buying price
 444                      + sum(p1, y(p1,t)*fuel_amount*(-fuel_price/1000))                                                               //Amount of fuel produced times selling price of fuel
 445                      + sum(i, (b(i,t)-s(i,t))*ALPHA(i)/1000)                                                                                        //Amount of extra material times its selling price
 446                      - sum(j, y(j,t)*c(j)/1000)
 447                      - sum(m, Cap(m,t)*FCost(m)/1000)                                                                                      //Amount of produced products times the production cost
 448                      ;
 449  // ======Sales Distribution among regions in each year=====//
 450  TotalSales(t)..  TotalSell(t) =E= (sum((k,j), (GAMMA(j,k)/1000) * sum(l, q(l,j)*u(l,j,k,t)))
 451                   - sum((k,j), (DELTA(j,k)/(1000*1000)) * sum(l, q(l,j)*q(l,j)
 452                           * u(l,j,k,t))/power(demand_growth(j), ord(t)-1)));
 453   
 454   
 455   
 456  // =======Overview======== //
 457  Ex1(t).. ATO(t) =e= sum((l,k,j), q(l,j)*u(l,j,k,t));
 458  Ex2(t).. DPC(t) =e= sum((n,i), h(n,i)*r(n,i,t)) + sum(j, y(j,t)*c(j)/1000);
 459  Ex3(t)..  SP(t) =e=  (sum((k,j), (GAMMA(j,k)/1000) * sum(l, q(l,j)*u(l,j,k,t)))
 460              - sum((k,j), (DELTA(j,k)/(1000*1000)) * sum(l, q(l,j)*q(l,j) * u(l,j,k,t))/power(demand_growth(j), ord(t)-1)));
 461  Ex4(t)..  FC(t) =e=  sum(m, Cap(m,t)*FCost(m)/1000) ;
 462  Ex5(t).. PROFIT(t) =e= power(0.95, ord(t)-1)*Pr(t);
 463   
 464   
 465  RegionSales(t,k).. RegionSell(t,k) =E= (sum((j), (GAMMA(j,k)/1000) * sum(l, q(l,j)*u(l,j,k,t)))
 466                   - sum((j), (DELTA(j,k)/(1000*1000)) * sum(l, q(l,j)*q(l,j)
 467                           * u(l,j,k,t))/power(demand_growth(j), ord(t)-1)));
